{
  "name": "@stackup/validate",
  "version": "0.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/validate.esm.js",
  "typings": "dist/index.d.ts",
  "description": "Functional, tree-shakeable, schema validation",
  "repository": "github:rzane/validate",
  "homepage": "https://github.com/rzane/validate#readme",
  "bugs": "https://github.com/rzane/validate/issues",
  "engines": {
    "node": ">=10"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint src test",
    "prepare": "tsdx build",
    "docs": "doctoc README.md",
    "size": "size-limit"
  },
  "prettier": {
    "arrowParens": "avoid",
    "trailingComma": "none"
  },
  "size-limit": [
    {
      "path": "dist/validate.esm.js",
      "limit": "4 KB"
    },
    {
      "path": "dist/validate.cjs.production.min.js",
      "limit": "4 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.2",
    "doctoc": "^2.0.0",
    "size-limit": "^4.10.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.3"
  }
}
